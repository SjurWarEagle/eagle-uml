{
  "name": "plantuml-client",
  "version": "0.0.0",
  "scripts": {
    "docker:build": "docker-compose -f ../docker/docker-compose.yml build picoweb",
    "docker:up": "docker-compose -f ../docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f ../docker/docker-compose.yml down",
    "docker:ps": "docker-compose -f ../docker/docker-compose.yml ps",
    "docker:ps_classic": "docker ps",
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "start2": "ng serve --host 0.0.0.0 --disable-host-check --port 4310",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier -w src",
    "git": "run-s git:add git:commit git:push",
    "git:commit": "git commit -m \"update dependencies\"",
    "git:add": "git add package.json package-lock.json",
    "git:push": "git push",
    "dependencies:update": "ncu -u -x typescript",
    "dependencies:install": "npm install",
    "update:dependencies and build": "run-s dependencies:update dependencies:install build git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.0.1",
    "@angular/common": "13.0.1",
    "@angular/compiler": "13.0.1",
    "@angular/core": "13.0.1",
    "@angular/forms": "13.0.1",
    "@angular/platform-browser": "13.0.1",
    "@angular/platform-browser-dynamic": "13.0.1",
    "@angular/router": "13.0.1",
    "rxjs": "7.4.0",
    "axios": "0.24.0",
    "tslib": "2.3.1",
    "nosleep.js": "0.12.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.2",
    "@angular/cli": "13.0.2",
    "@angular/compiler-cli": "13.0.1",
    "@types/node": "16.11.7",
    "codelyzer": "6.0.2",
    "npm-run-all": "4.1.5",
    "ts-node": "10.4.0",
    "tslint": "6.1.0",
    "npm-check-updates": "12.0.2",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  }
}
